{
  "schemaVersion": 1,
  "id": "copperbucket",
  "version": "${version}",

  "name": "CopperBucket",
  "description": "",
  "authors": [],
  "contact": {},

  "license": "All-Rights-Reserved",
  "icon": "assets/copperbucket/icon.png",

  "environment": "client",
  "entrypoints": {
    "fabric-datagen": ["org.rc.copperbucket.client.CopperbucketDataGenerator"],
    "client": ["org.rc.copperbucket.client.CopperbucketClient"],
